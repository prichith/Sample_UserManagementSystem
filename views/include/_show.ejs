
<% for(var i=0;i<users.length;i++) {%>
<tr>
    <th><%= i+1 %></th>
    <th><%= users[i].name %></th>
    <th><%= users[i].email%></th>
    <th><%= users[i].gender%></th>
    <th><%= users[i].status%></th>
    <th><a href="/update-user?id=<%=users[i]._id%>" class="btn border-shadow update"><span class="material-symbols-outlined">edit</span></a>
    <a href="/" class="btn border-shadow delete" data-id = <%= users[i]._id %> ><span class="material-symbols-outlined">delete</span></a></th>
</tr> 
<%} %>